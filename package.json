{
  "name": "@data-wise/atlas",
  "version": "0.3.1",
  "description": "Project state engine - registry, sessions, capture, and context management for ADHD-friendly workflow",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "atlas": "./bin/atlas.js"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "bin",
    "src",
    "completions",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "npm test -- --testPathPattern=test/unit",
    "test:integration": "npm test -- --testPathPattern=test/integration",
    "test:e2e": "npm test -- --testPathPattern=test/e2e",
    "test:coverage": "npm test -- --coverage",
    "test:dogfood": "bash test/dogfood.sh",
    "test:dogfood-auto": "bash test/dogfood-comprehensive.sh",
    "test:dogfood-log": "bash test/dogfood-comprehensive.sh --log",
    "test:dogfood-ui": "bash test/dogfood-interactive-v2.sh",
    "test:dashboard": "bash test/interactive-dashboard-test.sh",
    "lint": "eslint src/ test/",
    "format": "prettier --write 'src/**/*.js' 'test/**/*.js'",
    "start": "node bin/atlas.js",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "project-management",
    "workflow",
    "adhd",
    "productivity",
    "cli",
    "state-management",
    "context-switching",
    "session-tracking"
  ],
  "author": "Stat-Wise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Data-Wise/atlas.git"
  },
  "bugs": {
    "url": "https://github.com/Data-Wise/atlas/issues"
  },
  "homepage": "https://github.com/Data-Wise/atlas#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "open": "^11.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.7.4"
  }
}
